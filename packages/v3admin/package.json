  "author": "Sebastian Klocke",
  "license": "MIT",
  "scripts": {
    "build": "tsc -b",
    "watch": "tsc -b -w",
    "build-vue": "vue-cli-service build --name admin --target lib src/index.ts",
    "watch-vue": "vue-cli-service build --watch --name admin --target lib src/index.ts",
    "build-rollup": "rollup -c",
    "vue-help": "vue-cli-service build --help"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "keywords": [
    "vue",
    "vue 3",
    "vue3",
    "admin"
  ],
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@mdi/font": "5.9.55",
    "roboto-fontface": "*",
    "tslib": "^2.3.1",
    "vue": "3.2.19",
    "vue-router": "4.0.11",
    "vuetify": "3.0.0-alpha.11"
  },
  "devDependencies": {
    "@babel/types": "^7.15.6",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "^4.5.13",
    "rollup": "^2.58.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "~4.1.5"
  }
}
